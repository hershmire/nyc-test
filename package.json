{
  "name": "nyc-test",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/hershmire/nyc-test.git",
  "license": "MIT",
  "scripts": {
    "coverage": "nyc mocha src/**/test-node/**/*.js"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "cache": false,
    "all": true,
    "exclude": [
      "**/test-node/**",
      "!src/**/node_modules/**"
    ],
    "reporter": [
      "json",
      "text",
      "text-summary"
    ],
    "require": [
      "./babel-setup.js"
    ],
    "extension": [
      ".jsx",
      ".js"
    ]
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "mocha": "5.2.0",
    "nyc": "15.1.0"
  },
  "dependencies": {
    "react": "16.4.2",
    "react-dom": "16.4.2"
  }
}
